{
  "name": "mi18n",
  "version": "0.3.3",
  "description": "A simple yet robust i18n solution for universal JavaScript apps.",
  "main": "dist/mi18n.min.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --compilers js:babel-core/register ./src/**/*.test.js",
    "build": "webpack --progress -p",
    "start": "webpack -d | webpack-dev-server -d",
    "tag": "babel-node tools/run tag"
  },
  "repository": {
    "url": "http://github.com/draggable/mi18n",
    "type": "git"
  },
  "keywords": [
    "i18n"
  ],
  "author": "Kevin Chappell <kevin.b.chappell@gmail.com> (http://kevin-chappell.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Draggable/mi18n/issues"
  },
  "homepage": "https://github.com/Draggable/mi18n",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-regenerator-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.4.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-mocha": "^4.9.0",
    "json-update": "^3.0.0",
    "mocha": "^3.2.0",
    "replace-in-file": "^2.5.0",
    "webpack": "^2.4.1",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-dev-server": "^2.4.4"
  },
  "eslintConfig": {
    "plugins": [
      "mocha"
    ],
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "quote-props": 0,
      "no-unused-vars": 1,
      "no-console": 0,
      "new-cap": 0,
      "strict": 0,
      "arrow-parens": 0,
      "no-underscore-dangle": 0,
      "comma-dangle": 0,
      "no-use-before-define": 0,
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {}
}
